   <PIN pinname="ConfigShader" slicecount="1" values="|#include &lt;packs\PBRRenderer\nodes\modules\fxh\GBuffer_Common.fxh&gt;&cr;&lf;#ifndef GBUFFERCOMMON&cr;&lf;&tab;#include &lt;packs\PBRRenderer\nodes\modules\fxh\GBuffer_Common.fxh&gt;&cr;&lf;#endif&cr;&lf;&cr;&lf;#ifndef VERTEXFUNCTION&cr;&lf;#define VERTEXFUNCTION&cr;&lf;#endif&cr;&lf;&cr;&lf;//Available Uniform Parameters&cr;&lf;//&cr;&lf;//float Time&cr;&lf;//float DeltaTime&cr;&lf;&cr;&lf;//Available Matelial Data&cr;&lf;//float4 colorTex(float2 uv)&cr;&lf;//float metalTex(float2 uv)&cr;&lf;//float roughTex(float2 uv)&cr;&lf;//float3 emissionTex(float2 uv)&cr;&lf;//float Reflectance;&cr;&lf;&cr;&lf;//struct Light{&cr;&lf;//&tab;float3 pos;&cr;&lf;//&tab;float3 amb;&cr;&lf;//&tab;float3 diff;&cr;&lf;//&tab;float4x4 VP;&cr;&lf;//};&cr;&lf;//StructuredBuffer&lt;Light&gt; lights : Light List;&cr;&lf;&cr;&lf;//Input Data Struct&cr;&lf;//struct Info{&cr;&lf;//&tab;float3 Pos;&cr;&lf;//  float2 posScreen;&cr;&lf;//&tab;float3 camPos;&cr;&lf;//&tab;float3 Normal;&cr;&lf;//  float2 uv;&cr;&lf;//  float2 Velocity;&cr;&lf;//  float3 vertexColor; : if vertex color is none : vertexColor = 1.0;&cr;&lf;//};&cr;&lf;&cr;&lf;//Output Data Struct&cr;&lf;//struct OutputData{&cr;&lf;//&tab;float3 Albedo;&cr;&lf;//&tab;float3 Emission;&cr;&lf;//&tab;float Metalness;&cr;&lf;//&tab;float Roughness;&cr;&lf;//  float3 Normal; : Already Set World Normal&cr;&lf;//&tab;float Reflectance;&cr;&lf;//&tab;&cr;&lf;//&tab;float2 uv; : Already Set Model Texture Coordinate&cr;&lf;//};&cr;&lf;&cr;&lf;#include &lt;packs\PBRRenderer\nodes\modules\fxh\NoiseFunc.fxh&gt;&cr;&lf;&cr;&lf;float wave : WAVE;&cr;&lf;&cr;&lf;void VertexFunction(inout VertexData v){&cr;&lf;&tab;v.Pos.y += sin(length(v.Pos*2)+Time*-1.5+.5)*pow(wave, 2)*1;&lf;}&cr;&lf;&cr;&lf;void PostFunction(Info i, inout OutputData o){&cr;&lf;&tab;//Write your own post function!!&cr;&lf;&tab;&cr;&lf;&tab;float fresnel = 1 - saturate(dot(-normalize(i.Pos - i.camPos), i.Normal)); &cr;&lf;&cr;&lf;&tab;o.Albedo = colorTex(i.uv).rgb * (1+pow(fresnel, 5)*3);&cr;&lf;&tab;o.Emission = 0;&cr;&lf;&tab;o.Metalness = metalTex(i.uv);&cr;&lf;&tab;o.Roughness = roughTex(i.uv);&cr;&lf;&tab;o.BumpNormal = normalTex(i.uv);&cr;&lf;&tab;o.Reflectance = Reflectance;&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;#ifndef VERTEXFUNCTION&cr;&lf;#define VertexFunction(v)&cr;&lf;#endif&cr;&lf;&cr;&lf;float emit &lt;string uiname = &quot;Emission Stlength&quot;;&gt; = 1.0;&cr;&lf;&cr;&lf;SamplerState linearSampler : IMMUTABLE&cr;&lf;{&cr;&lf;&tab;Filter = MIN_MAG_MIP_LINEAR;&cr;&lf;&tab;AddressU = Wrap;&cr;&lf;&tab;AddressV = Wrap;&cr;&lf;};&cr;&lf; &cr;&lf;cbuffer cbPerDraw : register( b0 )&cr;&lf;{&cr;&lf;&tab;float4x4 tVP : VIEWPROJECTION;&tab;&cr;&lf;&tab;float4x4 tV : VIEW;&cr;&lf;&tab;float4x4 tVI : VIEWINVERSE;&cr;&lf;&tab;float4x4 ptVP : PREVIOUSVIEWPROJECTION;&cr;&lf;};&cr;&lf;&cr;&lf;cbuffer cbPerObj : register( b1 )&cr;&lf;{&cr;&lf;&tab;float4x4 tW : WORLD;&cr;&lf;&tab;float4x4 tWIT: WORLDLAYERINVERSETRANSPOSE;&cr;&lf;&tab;float4x4 ptW;&cr;&lf;&tab;float4x4 texW;&cr;&lf;};&cr;&lf;&cr;&lf;struct VS_IN&cr;&lf;{&cr;&lf;&tab;float4 PosO : POSITION;&cr;&lf;&tab;float4 TexCd : TEXCOORD0;&cr;&lf;&tab;float3 Norm : NORMAL;&cr;&lf;&tab;float3 tangent : TANGENT;&cr;&lf;&tab;uint vid : SV_VertexID;&cr;&lf;&tab;#if USECOLOR&cr;&lf;&tab;&tab;float3 color : COLOR;&cr;&lf;&tab;#endif&cr;&lf;&tab;#if USEVELOCITY&cr;&lf;&tab;&tab;float3 velocity : VELOCITY;&cr;&lf;&tab;#endif&cr;&lf;};&cr;&lf;&cr;&lf;struct VS_INTNB&cr;&lf;{&cr;&lf;&tab;float4 PosO : POSITION;&cr;&lf;&tab;float4 TexCd : TEXCOORD0;&cr;&lf;&tab;float3 Norm : NORMAL;&cr;&lf;&tab;uint vid : SV_VertexID;&cr;&lf;&tab;#if USECOLOR&cr;&lf;&tab;&tab;float3 color : COLOR;&cr;&lf;&tab;#endif&cr;&lf;&tab;#if USEVELOCITY&cr;&lf;&tab;&tab;float3 velocity : VELOCITY;&cr;&lf;&tab;#endif&cr;&lf;};&cr;&lf;&cr;&lf;struct vs2ps&cr;&lf;{&cr;&lf;&tab;float4 PosWVP: SV_Position;&cr;&lf;&tab;float4 PosW : TEXCOORD1;&cr;&lf;&tab;float4 velocity : TEXCOORD2;&cr;&lf;&tab;float4 uv: TEXCOORD0;&cr;&lf;&tab;float3 tangent : TEXCOORD3;&cr;&lf;&tab;float3 NormW : NORMAL;&cr;&lf;&tab;#if USECOLOR&cr;&lf;&tab;&tab;float3 color : COLOR;&cr;&lf;&tab;#endif&cr;&lf;};&cr;&lf;&cr;&lf;struct vs2pstnb&cr;&lf;{&cr;&lf;&tab;float4 PosWVP: SV_Position;&cr;&lf;&tab;float4 PosW : TEXCOORD1;&cr;&lf;&tab;float4 velocity : TEXCOORD2;&cr;&lf;&tab;float4 uv: TEXCOORD0;&cr;&lf;&tab;float3 NormW : NORMAL;&cr;&lf;&tab;#if USECOLOR&cr;&lf;&tab;&tab;float3 color : COLOR;&cr;&lf;&tab;#endif&cr;&lf;};&cr;&lf;&cr;&lf;struct PSout{&cr;&lf;&tab;float4 color : SV_Target0;&cr;&lf;&tab;float4 normal : SV_Target1;&cr;&lf;&tab;float4 MRVel : SV_Target2;&cr;&lf;&tab;float3 position : SV_Target3;&cr;&lf;};&cr;&lf;&cr;&lf;vs2ps VS(VS_IN input)&cr;&lf;{&cr;&lf;&tab;vs2ps output;&cr;&lf;&cr;&lf;&tab;VertexData v;&cr;&lf;&tab;v.Pos = input.PosO.xyz;&cr;&lf;&tab;v.Normal = input.Norm;&cr;&lf;&tab;VertexFunction(v);&cr;&lf;&cr;&lf;&tab;float4 modifiedPos = float4(v.Pos, 1);&cr;&lf;&tab;output.PosWVP  = mul(modifiedPos,mul(tW,tVP));&cr;&lf;&tab;float4 PosW = mul(modifiedPos, tW);&cr;&lf;&tab;&cr;&lf;&tab;float4 velpos = modifiedPos;&cr;&lf;&tab;#if USEVELOCITY&cr;&lf;&tab;&tab;velpos -= float4(input.velocity, 0);&cr;&lf;&tab;#endif&cr;&lf;&tab;float4 velocity = mul(velpos, ptW);&cr;&lf;&tab;output.PosW = PosW;&cr;&lf;&tab;&cr;&lf;&tab;float4 posV = mul(PosW, tVP);&cr;&lf;&tab;float4 vel = mul(velocity, ptVP);&cr;&lf;&tab;output.velocity = vel;&cr;&lf;&tab;&cr;&lf;&tab;output.uv = mul(input.TexCd, texW);;&cr;&lf;&tab;output.NormW = normalize(mul(v.Normal, (float3x3)tWIT));&cr;&lf;&tab;output.tangent = normalize(mul(input.tangent, (float3x3)tW).xyz);&cr;&lf;&tab;#if USECOLOR&cr;&lf;&tab;&tab;output.color =  input.color;&cr;&lf;&tab;#endif&cr;&lf;&tab;return output;&cr;&lf;}&cr;&lf;&cr;&lf;vs2pstnb VS_TNB(VS_INTNB input)&cr;&lf;{&cr;&lf;&tab;vs2pstnb output;&cr;&lf;&cr;&lf;&tab;VertexData v;&cr;&lf;&tab;v.Pos = input.PosO.xyz;&cr;&lf;&tab;v.Normal = input.Norm;&cr;&lf;&tab;VertexFunction(v);&cr;&lf;&cr;&lf;&tab;float4 modifiedPos = float4(v.Pos, 1);&cr;&lf;&tab;output.PosWVP  = mul(modifiedPos,mul(tW,tVP));&cr;&lf;&tab;float4 PosW = mul(modifiedPos, tW);&cr;&lf;&tab;&cr;&lf;&tab;float4 velpos = modifiedPos;&cr;&lf;&tab;#if USEVELOCITY&cr;&lf;&tab;&tab;velpos -= float4(input.velocity, 0);&cr;&lf;&tab;#endif&cr;&lf;&tab;float4 velocity = mul(velpos, ptW);&cr;&lf;&tab;output.PosW = PosW;&cr;&lf;&tab;&cr;&lf;&tab;float4 posV = mul(PosW, tVP);&cr;&lf;&tab;float4 vel = mul(velocity, ptVP);&cr;&lf;&tab;output.velocity = vel;&cr;&lf;&tab;&cr;&lf;&tab;output.uv = mul(input.TexCd, texW);&cr;&lf;&tab;output.NormW = normalize(mul(v.Normal, (float3x3)tWIT));&cr;&lf;&tab;#if USECOLOR&cr;&lf;&tab;&tab;output.color =  input.color;&cr;&lf;&tab;#endif&cr;&lf;&tab;return output;&cr;&lf;}&cr;&lf;&cr;&lf;PSout PS(vs2ps In){&cr;&lf;&tab;&cr;&lf;&tab;PSout gbuffer;&cr;&lf;&tab;Info info = (Info)0;&cr;&lf;&tab;OutputData o = (OutputData)0;&cr;&lf;&tab;&cr;&lf;&tab;float4 posvp = mul(In.PosW, tVP);&cr;&lf;&tab;float2 possc = In.PosWVP.xy / In.PosWVP.w;&cr;&lf;&tab;float2 velxy = possc - (In.velocity / In.velocity.w).xy;&cr;&lf;&tab;velxy *= .5;&cr;&lf;&tab;velxy += .5;&cr;&lf;&tab;&cr;&lf;&tab;info.Pos = In.PosW.xyz;&cr;&lf;&tab;info.posScreen = possc;&cr;&lf;&tab;info.camPos = tVI[3].xyz;&cr;&lf;&tab;info.Normal = In.NormW;&cr;&lf;&tab;info.uv = o.uv = In.uv.xy;&cr;&lf;&tab;info.Velocity = velxy * 2 - 1;&cr;&lf;&tab;info.vertexColor = float3(1,1,1);&cr;&lf;&tab;#if USECOLOR&cr;&lf;&tab;&tab;info.vertexColor = In.color;&cr;&lf;&tab;#endif&cr;&lf;&tab;&cr;&lf;&tab;PostFunction(info, o);&cr;&lf;&cr;&lf;&tab;float3 bNorm = In.NormW;&cr;&lf;&tab;if(IsBump){&cr;&lf;&tab;&tab;float3 bumpy = o.BumpNormal;&cr;&lf;&tab;&tab;bNorm = Bumps(info.Normal, In.tangent, bumpy);&cr;&lf;&tab;}&cr;&lf;&tab;&cr;&lf;&tab;float3 emissive = o.Emission * emit;&cr;&lf;&tab;gbuffer.color = float4(o.Albedo + emissive, 1);&cr;&lf;&tab;float4 norm = float4(In.NormW, o.Reflectance);&cr;&lf;&tab;gbuffer.normal = norm;&cr;&lf;&tab;&cr;&lf;&tab;gbuffer.MRVel = float4(o.Metalness, o.Roughness, velxy);&cr;&lf;&tab;gbuffer.position = In.PosW.xyz;&cr;&lf;&tab;gbuffer.normal = float4(bNorm, o.Reflectance);&cr;&lf;&cr;&lf;&tab;return gbuffer;&cr;&lf;}&cr;&lf;&cr;&lf;PSout PS_TNB(vs2pstnb In){&cr;&lf;&tab;PSout gbuffer;&cr;&lf;&tab;Info info = (Info)0;&cr;&lf;&tab;OutputData o = (OutputData)0;&cr;&lf;&tab;&cr;&lf;&tab;float4 posvp = mul(In.PosW, tVP);&cr;&lf;&tab;float2 possc = posvp.xy / posvp.w;&cr;&lf;&tab;float2 velxy = possc - (In.velocity / In.velocity.w).xy;&cr;&lf;&tab;velxy *= .5;&cr;&lf;&tab;velxy += .5;&cr;&lf;&tab;&cr;&lf;&tab;info.Pos = In.PosW.xyz;&cr;&lf;&tab;info.posScreen = possc;&cr;&lf;&tab;info.camPos = tVI[3].xyz;&cr;&lf;&tab;info.Normal = In.NormW;&cr;&lf;&tab;info.uv = o.uv = In.uv.xy;&cr;&lf;&tab;info.Velocity = velxy * 2 - 1;&cr;&lf;&tab;info.vertexColor = float3(1,1,1);&cr;&lf;&tab;#if USECOLOR&cr;&lf;&tab;&tab;info.vertexColor = In.color;&cr;&lf;&tab;#endif&cr;&lf;&tab;&cr;&lf;&tab;PostFunction(info, o);&cr;&lf;&cr;&lf;&tab;float3 bNorm = In.NormW;&cr;&lf;&tab;if(IsBump){&cr;&lf;&tab;&tab;float3 bumpy = o.BumpNormal;&cr;&lf;&tab;&tab;bNorm = BumpsTNB(info.Normal, bumpy, info.Pos, info.uv);&cr;&lf;&tab;}&cr;&lf;&tab;&cr;&lf;&tab;float3 emissive = o.Emission * emit;&cr;&lf;&tab;gbuffer.color = float4(o.Albedo + emissive, 1);&cr;&lf;&tab;float4 norm = float4(In.NormW, o.Reflectance);&cr;&lf;&tab;gbuffer.normal = norm;&cr;&lf;&tab;&cr;&lf;&tab;gbuffer.MRVel = float4(o.Metalness, o.Roughness, velxy);&cr;&lf;&tab;gbuffer.position = In.PosW.xyz;&cr;&lf;&tab;gbuffer.normal = float4(bNorm, o.Reflectance);&cr;&lf;&cr;&lf;&tab;return gbuffer;&cr;&lf;}&cr;&lf;&cr;&lf;technique10 GBuffer&cr;&lf;{&cr;&lf;&tab;pass P0&cr;&lf;&tab;{&cr;&lf;&tab;&tab;SetVertexShader( CompileShader( vs_4_0, VS() ) );&cr;&lf;&tab;&tab;SetPixelShader( CompileShader( ps_4_0, PS() ) );&cr;&lf;&tab;}&cr;&lf;}&cr;&lf;&cr;&lf;technique10 GBufferNoTangent&cr;&lf;{&cr;&lf;&tab;pass P0&cr;&lf;&tab;{&cr;&lf;&tab;&tab;SetVertexShader( CompileShader( vs_4_0, VS_TNB() ) );&cr;&lf;&tab;&tab;SetPixelShader( CompileShader( ps_4_0, PS_TNB() ) );&cr;&lf;&tab;}&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;|">
